SELECT I.SUMMARY AS TASK_NAME, L.NAME AS LIST_NAME
FROM ISSUES I
JOIN ISSUELISTS L ON I.ISSUELIST_ID = L.ID;

SELECT I.SUMMARY AS TASK_NAME, U.FIRSTNAME AS FIRSTNAME, U.LASTNAME AS LASTNAME
FROM ISSUES I
JOIN USERS U ON I.USER_ID_ASSIGNEDTO = U.ID;

SELECT U.FIRSTNAME AS FIRSTNAME, U.LASTNAME AS LASTNAME, COUNT(*) AS TASKS_NUMBER
FROM USERS U
JOIN ISSUES I ON U.ID = I.USER_ID_ASSIGNEDTO
GROUP BY U.FIRSTNAME, U.LASTNAME;